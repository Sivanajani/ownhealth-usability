diff --git a/app/src/pages/onboarding/CircularMenu.scss b/app/src/pages/onboarding/CircularMenu.scss
new file mode 100644
--- /dev/null	(date 1766884407209)
+++ b/app/src/pages/onboarding/CircularMenu.scss	(date 1766884407209)
@@ -0,0 +1,149 @@
+$items: 8;
+$radius: 200px;
+$box-size: 65px;
+$colors:
+        rgba(59,130,246),
+        rgba(239,68,68),
+        rgba(59,130,246),
+        rgba(245,158,11),
+        rgba(147,197,253),
+        rgba(236,72,153),
+        rgba(139,92,246),
+        rgba(16,185,129);
+
+.menu-container {
+  position: relative;
+  width: 100%;
+  height: 400px;
+  display: flex;
+  justify-content: center;
+  align-items: center;
+  margin: 20px 0;
+}
+
+.center-hub {
+  width: 90px;
+  height: 90px;
+  background: #252e42;
+  border: 1px solid rgba(255, 255, 255, 0.1);
+  border-radius: 22px;
+  display: flex;
+  justify-content: center;
+  align-items: center;
+  z-index: 10;
+  box-shadow: 0 0 30px rgba(0, 163, 255, 0.65);
+
+  .logo-text {
+    color: white;
+    font-weight: bold;
+    font-size: 1.4rem;
+    letter-spacing: 1px;
+  }
+}
+
+.circle-wrapper {
+  position: absolute;
+  inset: 0;
+  display: flex;
+  justify-content: center;
+  align-items: center;
+}
+
+.menu-item {
+  position: absolute;
+  width: $box-size;
+  height: $box-size;
+  display: flex;
+  flex-direction: column;
+  align-items: center;
+  justify-content: flex-start;
+  opacity: 0;
+  animation: flyOut 0.8s cubic-bezier(0.175, 0.885, 0.32, 1.275) forwards;
+
+  &::before {
+    content: "";
+    position: absolute;
+    top: calc($box-size / 2);
+    left: 50%;
+
+    width: calc(#{$radius} - 45px);
+    height: 1px;
+
+    transform-origin: left center;
+    transform: rotate(180deg);
+
+    z-index: -1;
+  }
+
+  .menu-box {
+    width: 100%;
+    height: 100%;
+    border: 2px solid;
+    border-radius: 18px;
+    transition: all 0.3s ease;
+    display: flex;
+    justify-content: center;
+    align-items: center;
+
+    .menu-icon {
+      font-size: 2rem;
+      line-height: 0;
+    }
+  }
+
+  .menu-label {
+    position: absolute;
+    top: 120%;
+    color: #8a94a6;
+    font-size: 0.75rem;
+    font-weight: 500;
+    text-align: center;
+  }
+
+  @for $i from 1 through $items {
+    &:nth-child(#{$i}) {
+      $angle: ($i - 1) * (360 / $items) - 90;
+      --angle: #{$angle}deg;
+      animation-delay: #{$i * 0.1s};
+      $current-clr: nth($colors, $i);
+
+      .menu-box {
+        border-color: rgba(nth($colors, $i), 0.5);
+      }
+
+      // line
+      &::before {
+        background-image: repeating-linear-gradient(
+          to right,
+          $current-clr,
+          $current-clr 3px,
+          transparent 3px,
+          transparent 7px
+        );
+
+        z-index: 0;
+        transform: rotate(calc(180deg + #{$angle}deg));
+        mask-image: linear-gradient(
+            to right,
+            transparent 0px,
+            transparent calc($box-size / 2),
+            black 46px,
+            black 70%,
+            transparent 100%
+        );
+      }
+    }
+  }
+}
+
+@keyframes flyOut {
+  $angle: var(--angle);
+  0% {
+    opacity: 0;
+    transform: rotate($angle) translate(0px) rotate(calc(-1 * $angle)) scale(0);
+  }
+  100% {
+    opacity: 1;
+    transform: rotate($angle) translate($radius) rotate(calc(-1 * $angle)) scale(1);
+  }
+}
